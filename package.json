{
  "name": "eagle-bn-frontend",
  "version": "1.0.0",
  "description": "this is a front-end application for Barefoot nomad",
  "main": "index.js",
  "jest": {
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "publicPath": "./html-report",
          "filename": "report.html",
          "expand": true
        }
      ]
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "json",
      "text",
      "html"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint",
      "pre-push": "npm test"
    }
  },
  "scripts": {
    "start": "serve -s ./build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "dev": "webpack-dev-server --mode=development --config webpack.config.dev.js --hot",
    "dev:open": "webpack-dev-server --mode=development --config  webpack.config.dev.js --open --hot",
    "eslint": "eslint src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela/eagle-bn-frontend.git"
  },
  "author": "Team eagle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela/eagle-bn-frontend/issues"
  },
  "homepage": "https://github.com/andela/eagle-bn-frontend#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-html-reporters": "^1.2.0",
    "preset-react": "^1.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "serve": "^11.2.0"
  }
}
